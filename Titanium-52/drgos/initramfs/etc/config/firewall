config firewall general
        option security_level   no
        option service_count    3
        option schedule_count   0
        option rules_count      0
        option url_count        0
        option NextRulePos      1

config firewall service1
        option name     telnet
        option protocol tcp
        option port     23

config firewall service2
        option name     ftp
        option protocol tcp
        option port     21

config firewall service3
        option name     ping
        option protocol icmp
        option port     8

