# Don't Fast Forward FTP traffic
config fastforward ftp
	option proto		tcp
	option port		21
config fastforward sip
  option proto  udp
  option port   5060
config fastforward rtsp
  option proto  tcp
  option port   554
config fastforward tftp
  option proto  udp
  option port   69

#config logging
#	option file	/var/log/cmm.log
#	option command	0
#	option error	1
#       option warning	1
#	option info	1

#WiFi fastforward configuration
config wifi_fastforward
        option ifname   ath0

config wifi_fastforward
        option ifname   ath1

