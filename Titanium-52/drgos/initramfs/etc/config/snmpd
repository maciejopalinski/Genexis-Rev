config agent
	option agentaddress UDP:161
	option notifyauthfail 2

config view all
	option viewname all
	option type included
	option oid .1

config system
	option sysLocation	''
	option sysContact	''
	option sysName		''
	option sysServices	72
	option sysDescr		''
#	option sysObjectID	'1.2.3.4'

config exec
	option name	filedescriptors
	option prog	/bin/cat
	option args	/proc/sys/fs/file-nr
